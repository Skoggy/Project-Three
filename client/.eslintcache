d\r\n                                title={search.result.name}\r\n                                body=''\r\n                                amount={search.result.amount - amounts.amount}\r\n                            />\r\n                            <input type=\"number\"\r\n                                name=\"TakeStock\"\r\n                                id=\"TakeStock\"\r\n                                value={amounts.amount}\r\n                                onChange={onAmountChange} />\r\n                            <Button onClick={takeStock}>Take</Button>\r\n                            <div>\r\n                                <input type=\"text\"\r\n                                    name='note'\r\n                                    id='note'\r\n                                    value={note.sendNote}\r\n                                    onChange={onNoteChange} />\r\n                                <Button onClick={addNote}>Add Note</Button>\r\n\r\n                                <p>{note.note}</p>\r\n                            </div>\r\n                        </div >\r\n                        :\r\n                        <div>{noMatch}</div>}\r\n\r\n                </FilloutStyles >\r\n\r\n            </Container>\r\n        </div>\r\n    )\r\n}\r\n\r\n","C:\\Users\\chris\\Desktop\\stockProject\\project3\\Project-Three\\client\\src\\components\\LogoutButton.js",[],"C:\\Users\\chris\\Desktop\\stockProject\\project3\\Project-Three\\client\\src\\components\\Form.js",[],["136","137"],"C:\\Users\\chris\\Desktop\\stockProject\\project3\\Project-Three\\client\\src\\components\\Card.js",[],"C:\\Users\\chris\\Desktop\\stockProject\\project3\\Project-Three\\client\\src\\components\\Button.js",[],"C:\\Users\\chris\\Desktop\\stockProject\\project3\\Project-Three\\client\\src\\utils\\API.js",["138"],"\r\nimport axios from 'axios';\r\n\r\n\r\n\r\nconst url = 'http://localhost:3001/api/stocktypes'\r\n\r\nexport default {\r\n    delete: function (uuid) {\r\